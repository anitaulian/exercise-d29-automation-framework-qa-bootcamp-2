<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SmokeTestSuite" verbose="1">
    <parameter name="browser" value="firefox"/>
<!--    <parameter name="browser" value="firefox"/> kalau ini di komen akan run di chrome as default-->
    <test name="LoginTests">
        <groups>
            <run>
                <include name="smoke"/>
                <exclude name="p2"/>
<!--                <include name="smoke"/> ini artinya pilih skenario test yang smoke "include"-->
<!--                <exclude name="p2"/> tapi jangan yang p2 (alias p1 aja) "exclude"-->
            </run>
        </groups>
        <classes>
            <class name="dibimbing.tests.LoginTest"/>
        </classes>
    </test>
</suite>